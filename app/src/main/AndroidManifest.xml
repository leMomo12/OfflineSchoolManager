<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mnowo.offlineschoolmanager">

    <application
        android:name=".SchoolManagerApplication"
        android:allowBackup="true"
        android:icon="@mipmap/manso_512px_app_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/manso_512px_app_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.OfflineSchoolManager">
        <activity
            android:name="com.mnowo.offlineschoolmanager.core.feature_core.presentation.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.OfflineSchoolManager">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <receiver
            android:name=".core.feature_subject.add_subject.domain.use_case.util.ExamNotificationReceiver">
        </receiver>
    </application>

</manifest>